{
  "name": "cadali/symfony-test-app",
  "type": "project",
  "description": "Symfony test application for Cadabra bundle integration and benchmarking",
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "ext-pdo_sqlite": "*",
    "ext-sqlite3": "*",
    "cadabra/php": "*",
    "nyholm/psr7": "^1.8",
    "doctrine/dbal": "^3.0",
    "doctrine/doctrine-bundle": "^2.11",
    "doctrine/doctrine-migrations-bundle": "^3.3",
    "doctrine/orm": "^2.14|^3.0",
    "symfony/console": "^6.0|^7.0",
    "symfony/dotenv": "^6.0|^7.0",
    "symfony/flex": "^2",
    "symfony/framework-bundle": "^6.0|^7.0",
    "symfony/runtime": "^6.0|^7.0",
    "symfony/yaml": "^6.0|^7.0"
  },
  "require-dev": {
    "doctrine/doctrine-fixtures-bundle": "^3.5",
    "fakerphp/faker": "^1.23",
    "phpunit/phpunit": "^10.0",
    "symfony/phpunit-bridge": "^6.0|^7.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "symfony/flex": true,
      "symfony/runtime": true
    },
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "extra": {
    "symfony": {
      "allow-contrib": false
    }
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": ["@auto-scripts"],
    "post-update-cmd": ["@auto-scripts"]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "path",
      "url": "../",
      "options": {
        "symlink": false
      }
    }
  ]
}
